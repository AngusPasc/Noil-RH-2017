  object CdCargo: TZQuery
    Connection = connection.zConnection
    SQL.Strings = (
      'SELECT '
      '  c.idcargo, '
      '  c.IdOrganizacion, '
      '  c.IdTipoNomina, '
      '  c.codigocargo, '
      '  c.titulocargo, '
      '  c.activo, '
      '  c.iddocumentos,'
      '  c.jornada,'
      '  c.nivel, '
      '  c.idsalario, '
      '  c.Grupo, '
      '  c.IdRangoSalarios, '
      '  rs1.salario, '
      '  rs1.codigosalario, '
      '  rs1.titulosalario, '
      '  CONCAT(mon.simbolo, " ",rs1.salario) as monto, '
      
        '  Concat("[", rs1.codigosalario, "] -  ", rs1.titulosalario) as ' +
        'LeyendaTituloSalario, '
      '  rango.Importe,'
      '  rango.Titulo  '
      'FROM '
      '  nuc_cargo c '
      ''
      'left join '
      '  rhu_salario rs '
      '    on (rs.IdSalario = c.idsalario) '
      ''
      'left join '
      '  rhu_salario rs1 '
      '    on (rs1.idllave = rs.idllave and rs1.activo= "Si" and '
      '        rs1.fechaaplicacion =(Select '
      '                                max(x.fechaaplicacion) '
      '                              from '
      '                                rhu_salario x '
      '                              left join '
      '                                rhu_modificacionsalario y '
      
        '                                  on (y.IdSalario = x.IdSalario)' +
        ' '
      '                              where '
      
        '                                x.Activo = "Si" and  x.idLlave =' +
        ' rs1.IdLlave and '
      
        '                                ((:FechaAplicacion = -1 and x.fe' +
        'chaaplicacion <= now()) or '
      
        '                                (:FechaAplicacion <> -1 and x.fe' +
        'chaaplicacion <= :FechaAplicacion)) and '
      
        '                                (Isnull(y.Estado) or y.Estado <>' +
        ' "Pendiente")) ) '
      ''
      'left join '
      '  nuc_moneda mon '
      '    on (mon.IdMoneda = rs.IdMoneda) '
      ''
      'left join '
      '  nom_rangosalarios rango '
      '    on (rango.IdRangoSalarios = c.IdRangoSalarios) '
      'where'
      ' (:idcargo = -1 or (:idcargo <> -1 and c.idcargo = :idcargo))'
      'ORDER BY '
      '  c.codigocargo ')
    Params = <
      item
        DataType = ftUnknown
        Name = 'FechaAplicacion'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'idcargo'
        ParamType = ptUnknown
      end>
    Left = 80
    Top = 349
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'FechaAplicacion'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'idcargo'
        ParamType = ptUnknown
      end>
  end
