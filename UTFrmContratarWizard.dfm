  object CdOrganizacion: TZQuery
    Connection = connection.zConnection
    SQL.Strings = (
      'SELECT '
      '  o.idnivel,'
      '  o.padre,'
      '  o.idorganizacion,'
      '  o.codigoorganizacion,'
      '  o.nombreorganizacion,'
      '  o.tituloorganizacion,'
      '  o.descripcion,'
      '  o.comentarios,'
      '  o.telefono1,'
      '  o.telefono2,'
      '  o.telefono3,'
      '  o.telefono4,            '
      '  o.telefono5,'
      '  o.fecha,'
      '  o.rfc,   '
      '  o.wbs,        '
      '  p.codigoorganizacion as codigopadre,'
      '  o.domiciliofiscalorg,'
      '  o.domicilioorg, '
      '  o.regpatimss,'
      '  o.FactorPrimaDominical,'
      '  o.FactorPrimaVacacional,'
      '  o.DiasExcentoAguinaldo,'
      '  o.DiasExcentoPrimaVacacional,'
      '  o.SeguroVidaINFONAVIT,  '
      '  n.titulonivel,'
      ''
      '  /* Obtener el salario m'#195#402#194#173'nimo de la zona */'
      '  (select'
      '    (select '
      '      a.Salario'
      '    from'
      '      rhu_salario a '
      '    where'
      '      a.IdSalario = IFNull(b.IdSalario, c.IdSalario) and '
      
        '      a.fechaaplicacion = (Select max(x.fechaaplicacion) from rh' +
        'u_salario x left join rhu_modificacionsalario y on (y.IdSalario ' +
        '= x.IdSalario) where a.Activo = "Si" and a.idLlave = x.idLlave a' +
        'nd ((:FechaAplicacion = -1 and x.fechaaplicacion <= now()) or (:' +
        'FechaAplicacion <> -1 and x.fechaaplicacion <= :FechaAplicacion)' +
        ') and (Isnull(y.Estado) or y.Estado <> "Pendiente"))) as SalMinG' +
        'enZona'
      '  from'
      '    nuc_organizacion c '
      '  left join '
      '    nuc_organizacion b '
      '      on (b.IdOrganizacion = c.Padre)'
      '  where'
      '    c.IdOrganizacion = o.IdOrganizacion) as SalMinGenZona,'
      ''
      
        '  IF((select max(b.IdNivelOrganizacion) from nuc_nivelorganizaci' +
        'on b) = p.IdNivel, True, False) as EsConvenio  '
      'FROM '
      '  nuc_organizacion o         '
      'INNER JOIN '
      '  nuc_nivelorganizacion n'
      '    on (o.idnivel = n.idnivelorganizacion)'
      'left join '
      '  nuc_organizacion p '
      '    on (o.padre = p.idorganizacion)'
      'ORDER BY '
      '  o.IdNivel')
    Params = <
      item
        DataType = ftUnknown
        Name = 'FechaAplicacion'
        ParamType = ptUnknown
      end>
    Left = 16
    Top = 349
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'FechaAplicacion'
        ParamType = ptUnknown
      end>
  end
