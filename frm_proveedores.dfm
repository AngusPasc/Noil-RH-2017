  object Proveedores: TZQuery
    Connection = connection.zConnection
    AfterScroll = ProveedoresAfterScroll
    SQL.Strings = (
      'select * from proveedores '
      'where (:Id = -1 or (:Id <> -1 and sIdProveedor= :Id)) or'
      '(:Razon = -1 or (:Razon <> -1 and sRazon like :Razon)) or'
      
        '(:NomCorto = -1 or (:NomCorto <> -1 and sNombreCorto like :NomCo' +
        'rto))'
      'order by sRazon')
