  object GruposxPrograma: TZReadOnlyQuery
    Connection = connection.zConnection
    AfterScroll = GruposxProgramaAfterScroll
    OnCalcFields = GruposxProgramaCalcFields
    SQL.Strings = (
      'Select g.*, p.sDescripcion from gruposxprograma g '
      'inner join programas p on(p.sIdPrograma = g.sIdPrograma)'
      
        'Where g.sIdGrupo =:Grupo And p.sIdIdioma =:Idioma order by p.sDe' +
        'scripcion')
    Params = <
      item
        DataType = ftUnknown
        Name = 'Grupo'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'Idioma'
        ParamType = ptUnknown
      end>
    Left = 656
    Top = 320
    ParamData = <
      item
        DataType = ftUnknown
